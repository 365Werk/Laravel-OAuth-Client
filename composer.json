{
    "name": "werk365/laraveloauthclient",
    "description": "Multi purpose OAuth client to use in Laravel",
    "license": "MIT",
    "version": "1.0.3",
    "authors": [
        {
            "name": "Hergen Dillema",
            "email": "h.dillema@365werk.nl",
            "homepage": "https://365werk.nl"
        }
    ],
    "homepage": "https://github.com/365Werk/Laravel-OAuth-Client",
    "keywords": ["Laravel", "LaravelOAuthClient", "OAuth", "OAuthClient"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Werk365\\LaravelOAuthClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Werk365\\LaravelOAuthClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Werk365\\LaravelOAuthClient\\LaravelOAuthClientServiceProvider"
            ]
        }
    }
}
